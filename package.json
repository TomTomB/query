{
  "name": "tomtomb",
  "version": "0.0.0",
  "license": "MIT",
  "packageManager": "yarn@3.2.1",
  "workspaces": [
    "libs/**"
  ],
  "scripts": {
    "commit": "npx git-cz",
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "prepare": "husky install",
    "format": "nx format:write --base=origin/main --head=HEAD",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "update": "nx migrate latest",
    "release": "yarn prepare:release && yarn changeset publish",
    "prepare:release": "yarn config set no-git-checks true && sed -e \"s|libs/|dist/libs/|\" package.json > package-new.json && mv package-new.json package.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.6",
    "@angular/common": "13.3.6",
    "@angular/compiler": "13.3.6",
    "@angular/core": "13.3.6",
    "@angular/forms": "13.3.6",
    "@angular/platform-browser": "13.3.6",
    "@angular/platform-browser-dynamic": "13.3.6",
    "@angular/router": "13.3.6",
    "@changesets/cli": "2.22.0",
    "@nrwl/angular": "14.1.5",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.5",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "13.3.5",
    "@angular/compiler-cli": "13.3.6",
    "@angular/language-service": "13.3.6",
    "@babel/core": ">=7.0.1 <8.0.0",
    "@babel/preset-env": ">=7.0.0 <8.0.0",
    "@changesets/changelog-github": "0.4.4",
    "@nrwl/cli": "14.1.5",
    "@nrwl/cypress": "14.1.5",
    "@nrwl/eslint-plugin-nx": "14.1.5",
    "@nrwl/jest": "14.1.5",
    "@nrwl/js": "14.1.5",
    "@nrwl/linter": "14.1.5",
    "@nrwl/nx-cloud": "14.0.3",
    "@nrwl/workspace": "14.1.5",
    "@types/jest": "27.5.0",
    "@types/node": "17.0.31",
    "@typescript-eslint/eslint-plugin": "5.22.0",
